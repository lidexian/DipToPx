<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        
        <shape android:shape="rectangle">

            <!-- 纯色 -->
            <solid android:color="#00000000" />
            <!-- 描边 -->
            <stroke android:width="1dp" android:color="#4c4545" />

            <!-- 圆角 -->
            <corners android:bottomLeftRadius="4px" android:bottomRightRadius="4px" android:topLeftRadius="4px" android:topRightRadius="4px" />
        </shape>
        
    </item>

</selector>